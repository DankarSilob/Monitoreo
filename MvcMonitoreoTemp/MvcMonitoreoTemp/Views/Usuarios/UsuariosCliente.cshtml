@model IEnumerable<MvcMonitoreoTemp.Models.Usuario>

@{
    ViewBag.Title = "Index";
}

<h2>Index</h2>

<p>
    @Html.ActionLink("Agregar Usuario", "AgregarUsuario", new { id = ViewBag.idCliente })
</p>

<table>
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.nombre)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.login)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.password)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.nivel)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.fecha_registro)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.nombre)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.login)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.password)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.nivel)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.fecha_registro)
        </td>
        <td>
            @Html.ActionLink("Edit", "Edit", new { id=item.cve_usuario }) |
            @Html.ActionLink("Details", "Details", new { id=item.cve_usuario }) |
            @Html.ActionLink("Delete", "Delete", new { id=item.cve_usuario })
        </td>
    </tr>
}

</table>

<p>
    @Html.ActionLink("Regresar", "", "Clientes")
</p>
